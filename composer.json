{
    "name": "shops25/shops25",
    "description": "Camboida e-commerce on shopping everywhere you wanted",
    "keywords": ["Ecommerce", "Cambodia shopping", "Website ecommerce"],
    "homepage": "http://shops25.com",
    "license": "MIT",
    "authors": [{
        "name": "Reksmey SREY",
        "email": "reksmeysrey@gmail.com"
    }],
    "type": "project",
    "require": {
        "php": "^7.2.5",
        "aws/aws-sdk-php": "^3.0",
        "fideloper/proxy": "^4.2",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^7.0",
        "laravel/helpers": "^1.1",
        "laravel/passport": "^8.4",
        "laravel/tinker": "^2.0",
        "laravel/ui": "2.0",
        "lorisleiva/laravel-deployer": "^0.3.2",
        "paypal/rest-api-sdk-php": "^1.13",
        "phpoffice/phpspreadsheet": "^1.8",
        "spatie/laravel-json-api-paginate": "^1.7",
        "spatie/laravel-pjax": "^2.0",
        "unisharp/laravel-filemanager": "^2.0"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "barryvdh/laravel-debugbar": "^3.2",
        "filp/whoops": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },

    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}